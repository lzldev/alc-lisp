(open "./examples/advent of code 2024/input/01.txt")

(def input
  (flat
   (map
    (lines (read_file))
    fn [v] ((def values (split v "   "))
            (map values parse_int)))))

(def (reduce input fn [acc v] ((def idx (nth 0 acc))
                               (def left (nth 0 (nth 1 acc)))
                               (def right (nth 1 (nth 1 acc))))
             [0, [[] []]]))

(print "input: " input)